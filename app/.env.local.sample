# Sample environment variables for ovpn-webapp
# Copy this file to `.env.local` and fill in the values for your environment

# Express server port
PORT=3000

# Azure Key Vault
KEY_VAULT_NAME=your-keyvault-name
CLIENT_ID=your-managed-identity-client-id

# Azure Key Vault secret names
SSH_SECRET_NAME=your-ssh-key-secret-name
CA_PASSWORD=your-ca-password-secret-name

# OpenVPN server details
OVPN_SERVER1_NAME=OpenVPN-Server-1
OVPN_SERVER1_IP_PUBLIC=1.2.3.4
OVPN_SERVER1_IP_PRIVATE=10.0.0.1

OVPN_SERVER2_NAME=OpenVPN-Server-2
OVPN_SERVER2_IP_PUBLIC=5.6.7.8
OVPN_SERVER2_IP_PRIVATE=10.0.0.2

OVPN_SERVER3_NAME=OpenVPN-Server-3
OVPN_SERVER3_IP_PUBLIC=9.10.11.12
OVPN_SERVER3_IP_PRIVATE=10.0.0.3

# SSH username for connecting to OpenVPN servers
SSH_USERNAME=ubuntu
